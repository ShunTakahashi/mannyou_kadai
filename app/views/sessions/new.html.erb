<h1>ログイン</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

  <%= f.email_field :email, placheolder: "メールアドレス" %>
  <br>
  <%= f.password_field :password, pleacheolder: "パスワード" %>
  <br>
  <%= f.submit "ログイン", class: "submit btn btn-success" %>

<% end %>
<%= link_to "新規登録", '/users/new' %>